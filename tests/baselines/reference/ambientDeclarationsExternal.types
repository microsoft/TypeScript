>     : ^^^^^^^^^^^

var n = imp3.x;
>n : number
>  : ^^^^^^
>imp3.x : number
>       : ^^^^^^
>imp3 : typeof imp3
>     : ^^^^^^^^^^^
>x : number
>  : ^^^^^^

var n: number;
>n : number
>  : ^^^^^^

=== decls.ts ===
// Ambient external module with export assignment
declare module 'equ' {
>'equ' : typeof import("equ")
>      : ^^^^^^^^^^^^^^^^^^^^

    var x;
>x : any

    export = x;
>x : any
>  : ^^^
}

declare module 'equ2' {
>'equ2' : typeof import("equ2")
>       : ^^^^^^^^^^^^^^^^^^^^^

    var x: number;
>x : number
>  : ^^^^^^
}

