>a : 2
>b : 4 | 3

    declare let s: S;
>s : S

    declare let t: T;
>t : T

    // S is assignable to T0 when S["a"] is 0
    // S is assignable to T2 when S["a"] is 2
    t = s;
>t = s : S
>t : T
>s : S
}

// Unmatched discriminants
namespace Example3 {
>Example3 : typeof Example3

    type S = { a: 0 | 2, b: 4 };
>S : S
>a : 0 | 2
>b : 4

    type T = { a: 0,     b: 1 | 4 }     // T0
>T : T
>a : 0
>b : 4 | 1

           | { a: 1,     b: 2 | 4 }     // T1
>a : 1
>b : 2 | 4

           | { a: 2,     b: 3 };        // T2
>a : 2
>b : 3

    declare let s: S;
>s : S

    declare let t: T;
>t : T

    // S is assignable to T0 when S["a"] is 0
    // S is *not* assignable to T1 when S["b"] is 4
    // S is *not* assignable to T2 when S["a"] is 2
    t = s;
>t = s : S
>t : T
>s : S
}

