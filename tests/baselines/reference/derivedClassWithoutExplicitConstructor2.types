>  : ^

    constructor(x: T, y?: T, z?: T);
>x : T
>  : ^
>y : T
>  : ^
>z : T
>  : ^

    constructor(x: T, y?: T);
>x : T
>  : ^
>y : T
>  : ^

    constructor(x: T) { this.a = x; }
>x : T
>  : ^
>this.a = x : T
>           : ^
>this.a : T
>       : ^
>this : this
>     : ^^^^
>a : T
>  : ^
>x : T
>  : ^
}

