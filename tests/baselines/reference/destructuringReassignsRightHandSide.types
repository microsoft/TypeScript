var foo: any = { foo: 1, bar: 2 };
>foo : any
>{ foo: 1, bar: 2 } : { foo: number; bar: number; }
>foo : number
>1 : 1
>bar : number
>2 : 2

var bar: any;
>bar : any

// reassignment in destructuring pattern
({ foo, bar } = foo);
>({ foo, bar } = foo) : any
>{ foo, bar } = foo : any
>{ foo, bar } : { foo: any; bar: any; }
>foo : any
>bar : any
