    Disjunction = "Disjunction",
>Disjunction : EnumTypeNode.Disjunction
>            : ^^^^^^^^^^^^^^^^^^^^^^^^
>"Disjunction" : "Disjunction"
>              : ^^^^^^^^^^^^^
}

type NodeA = Disjunction | Pattern;
>NodeA : NodeA
>      : ^^^^^

interface NodeBase {
    type: NodeA["type"]
>type : EnumTypeNode
>     : ^^^^^^^^^^^^
}

interface Disjunction extends NodeBase {
    type: EnumTypeNode.Disjunction
>type : EnumTypeNode.Disjunction
>     : ^^^^^^^^^^^^^^^^^^^^^^^^
>EnumTypeNode : any
>             : ^^^

    alternatives: string[]
>alternatives : string[]
