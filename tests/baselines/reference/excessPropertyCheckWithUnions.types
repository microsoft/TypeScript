// Freshness disappears after spreading a union
declare let t0: { a: any, b: any } | { d: any, e: any }
>t0 : { a: any; b: any; } | { d: any; e: any; }
>a : any
>b : any
>d : any
>e : any

declare let t1: { a: any, b: any, c: any } | { c: any, d: any, e: any }
