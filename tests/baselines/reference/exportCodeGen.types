// should replace all refs to 'x' in the body,
// with fully qualified
namespace A {
>A : typeof A
>  : ^^^^^^^^

    export var x = 12;
>x : number
>  : ^^^^^^
>12 : 12
>   : ^^

    function lt12() {
>lt12 : () => boolean
>     : ^^^^^^^^^^^^^

        return x < 12;
>x < 12 : boolean
>       : ^^^^^^^
>x : number
>  : ^^^^^^
>12 : 12
>   : ^^
    }
} 

// should not fully qualify 'x'
namespace B {
>B : typeof B
>  : ^^^^^^^^

    var x = 12;
>x : number
>  : ^^^^^^
>12 : 12
>   : ^^

    function lt12() {
>lt12 : () => boolean
>     : ^^^^^^^^^^^^^

        return x < 12;
>x < 12 : boolean
>       : ^^^^^^^
>x : number
>  : ^^^^^^
>12 : 12
>   : ^^
    }
}

