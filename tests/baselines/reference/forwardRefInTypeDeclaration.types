=== tests/cases/compiler/forwardRefInTypeDeclaration.ts ===
// forward ref ignored in a typeof
declare let s: typeof s1;
>s : "x"
>s1 : "x"

const s1 = "x";
>s1 : "x"
>"x" : "x"

// ignored anywhere in an interface (#35947)
interface Foo2 { [s2]: number; }
>[s2] : number
>s2 : "x"

const s2 = "x";
>s2 : "x"
>"x" : "x"

// or in a type definition
type Foo3 = { [s3]: number; }
>Foo3 : Foo3
>[s3] : number
>s3 : "x"

const s3 = "x";
>s3 : "x"
>"x" : "x"

