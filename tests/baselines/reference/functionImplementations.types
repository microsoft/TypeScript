>       : ^^^
>rec1 : () => any
>     : ^^^^^^^^^
}
var a = rec1();
>a : any
>  : ^^^
>rec1() : any
>       : ^^^
>rec1 : () => any
>     : ^^^^^^^^^

var a = rec2();
>a : any
>  : ^^^
>rec2() : any
>       : ^^^
>rec2 : () => any
>     : ^^^^^^^^^

