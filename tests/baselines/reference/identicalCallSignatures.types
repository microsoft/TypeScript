=== identicalCallSignatures.ts ===
// Each pair of call signatures in these types have a duplicate signature error.
// Identical call signatures should generate an error.
interface I {
    (x): number;
>x : any

    (x: any): number;
>x : any

    <T>(x: T): T;
>x : T
>  : ^

    <U>(x: U): U; // error
>x : U
>  : ^
}

interface I2<T> {
    (x: T): T;
>x : T
>  : ^

    (x: T): T; // error
>x : T
>  : ^
}

var a: {
>a : { (x: any): number; (x: any): number; <T>(x: T): T; <T>(x: T): T; }
>  : ^^^ ^^^^^^^^      ^^^ ^^   ^^^      ^^^ ^^ ^^ ^^^ ^^^ ^^ ^^ ^^^ ^^^

    (x): number;
>x : any

    (x: any): number;
>x : any

    <T>(x: T): T;
>x : T
>  : ^

    <T>(x: T): T; // error
>x : T
>  : ^
}
