declare const x: { a: 1, b: string };
>x : { a: 1; b: string; }
>a : 1
>b : string

declare const y: { a: number, b: 'a' };
>y : { a: number; b: 'a'; }
>a : number
>b : "a"

x === y;
>x === y : boolean
>x : { a: 1; b: string; }
>y : { a: number; b: "a"; }

