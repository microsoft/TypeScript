// All errors

// naked continue not allowed
continue;

// non-existent label
ONE:
>ONE : any
>    : ^^^

do continue TWO; while (true)
>TWO : any
>    : ^^^
>true : true
>     : ^^^^

// continue from inside function
TWO:
>TWO : any
>    : ^^^

do {
    var x = () => {
>x : () => void
>  : ^^^^^^^^^^
>() => {        continue TWO;    } : () => void
>                                  : ^^^^^^^^^^

        continue TWO;
>TWO : any
>    : ^^^
    }
}while (true)
>true : true
>     : ^^^^

THREE:
>THREE : any
>      : ^^^

do {
    var fn = function () {
