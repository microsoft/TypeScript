>                                          : ^^^^^^^^^^

        continue THREE;
>THREE : any
>      : ^^^
    }
}while (true)
>true : true
>     : ^^^^

// continue forward
do {
    continue FIVE;
>FIVE : any
>     : ^^^

    FIVE:
>FIVE : any
