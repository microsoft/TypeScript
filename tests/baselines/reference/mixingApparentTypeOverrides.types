type Constructor<T> = new(...args: any[]) => T;
>Constructor : Constructor<T>
>            : ^^^^^^^^^^^^^^
>args : any[]
>     : ^^^^^

function Tagged<T extends Constructor<{}>>(Base: T) {
>Tagged : <T extends Constructor<{}>>(Base: T) => T & { new (...args: any[]): (Anonymous class); prototype: Tagged<any>.(Anonymous class); }
>       : ^ ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^ ^^^^^^^^^^^^^^^^^^^^^^^^^     ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
>Base : T
>     : ^

  return class extends Base {
>class extends Base {    _tag: string;    constructor(...args: any[]) {      super(...args);      this._tag = "";    }  } : T & { new (...args: any[]): (Anonymous class); prototype: Tagged<any>.(Anonymous class); }
>                                                                                                                         : ^^^^^^^^^^^^^^^^^^^^     ^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^^
>Base : {}
>     : ^^

    _tag: string;
>_tag : string
>     : ^^^^^^

    constructor(...args: any[]) {
>args : any[]
>     : ^^^^^

      super(...args);
>super(...args) : void
>               : ^^^^
>super : T
>      : ^
>...args : any
>args : any[]
>     : ^^^^^

      this._tag = "";
>this._tag = "" : ""
>               : ^^
>this._tag : string
>          : ^^^^^^
>this : this
>     : ^^^^
>_tag : string
>     : ^^^^^^
>"" : ""
>   : ^^
    }
  };
}

class A {
>A : A
>  : ^

  toString () {
>toString : () => string
