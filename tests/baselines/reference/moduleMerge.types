=== tests/cases/compiler/moduleMerge.ts ===
// This should not compile both B classes are in the same module this should be a collision

module A
>A : typeof A
{
    class B
>B : B
    {
        public Hello(): string
>Hello : () => string
        {
            return "from private B";
>"from private B" : string
        }
    }
}

module A
>A : typeof A
{
    export class B
>B : B
    {
        public Hello(): string
>Hello : () => string
        {
            return "from export B";
>"from export B" : string
        }
    }
}
