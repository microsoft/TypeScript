=== good.js ===
/**
 * @param
 * {number} x Arg x.
 * @param {number}
 * y Arg y.
 * @param {number} z
 * Arg z.
 */
function good(x, y, z) {
>good : (x: number, y: number, z: number) => void
>     : ^ ^^      ^^ ^^      ^^ ^^      ^^^^^^^^^
>x : number
>  : ^^^^^^
>y : number
>  : ^^^^^^
>z : number
>  : ^^^^^^
}

good(1, 2, 3)
>good(1, 2, 3) : void
>              : ^^^^
>good : (x: number, y: number, z: number) => void
>     : ^ ^^      ^^ ^^      ^^ ^^      ^^^^^^^^^
>1 : 1
>  : ^
>2 : 2
>  : ^
>3 : 3
>  : ^


