// Class with protected members
class C1 {
>C1 : C1

    protected x: number;
>x : number

    protected static sx: number;
>sx : number

    protected f() {
>f : () => number

        return this.x;
>this.x : number
>this : this
>x : number
    }
    protected static sf() {
>sf : () => number

        return this.sx;
>this.sx : number
>this : typeof C1
>sx : number
    }
}

// Derived class accessing protected members
class C2 extends C1 {
