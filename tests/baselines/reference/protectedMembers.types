// Class with protected members
class C1 {
>C1 : C1
>   : ^^

    protected x: number;
>x : number
>  : ^^^^^^

    protected static sx: number;
>sx : number
>   : ^^^^^^

    protected f() {
>f : () => number
>  : ^^^^^^^^^^^^

        return this.x;
>this.x : number
>       : ^^^^^^
>this : this
>     : ^^^^
>x : number
>  : ^^^^^^
    }
    protected static sf() {
>sf : () => number
>   : ^^^^^^^^^^^^

        return this.sx;
>this.sx : number
>        : ^^^^^^
>this : typeof C1
>     : ^^^^^^^^^
>sx : number
>   : ^^^^^^
    }
}

// Derived class accessing protected members
class C2 extends C1 {
>C2 : C2
>   : ^^
>C1 : C1
>   : ^^

    protected f() {
>f : () => number
>  : ^^^^^^^^^^^^

        return super.f() + this.x;
>super.f() + this.x : number
>                   : ^^^^^^
>super.f() : number
>          : ^^^^^^
>super.f : () => number
>        : ^^^^^^^^^^^^
>super : C1
>      : ^^
>f : () => number
>  : ^^^^^^^^^^^^
>this.x : number
>       : ^^^^^^
>this : this
>     : ^^^^
>x : number
>  : ^^^^^^
    }
    protected static sf() {
>sf : () => number
>   : ^^^^^^^^^^^^

        return super.sf() + this.sx;
>super.sf() + this.sx : number
>                     : ^^^^^^
>super.sf() : number
>           : ^^^^^^
>super.sf : () => number
>         : ^^^^^^^^^^^^
>super : typeof C1
>      : ^^^^^^^^^
>sf : () => number
>   : ^^^^^^^^^^^^
>this.sx : number
>        : ^^^^^^
>this : typeof C2
>     : ^^^^^^^^^
>sx : number
>   : ^^^^^^
    }
}

