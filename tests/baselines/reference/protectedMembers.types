>this.x : number
>this : this
>x : number
    }
    protected static sf() {
>sf : () => number

        return super.sf() + this.sx;
>super.sf() + this.sx : number
>super.sf() : number
>super.sf : () => number
>super : typeof C1
>sf : () => number
>this.sx : number
>this : typeof C2
>sx : number
    }
}

