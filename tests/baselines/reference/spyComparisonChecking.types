type SpyObj<T> = T & {
>SpyObj : SpyObj<T>

    [k in keyof T]: Spy;
}

declare function createSpyObj<T>(
>createSpyObj : <T>(name: string, names: Array<keyof T>) => SpyObj<T>

    name: string, names: Array<keyof T>): SpyObj<T>;
>name : string
