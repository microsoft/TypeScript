let x: "foo";
>x : "foo"

let y: "foo" | "bar"; 
>y : "foo" | "bar"

switch (x) {
>x : "foo"

    case "foo":
>"foo" : "foo"

        break;
    case "bar":
>"bar" : "bar"

        break;
    case y:
>y : "foo" | "bar"

        y;
>y : "foo" | "bar"

        break;
}

