>1 : Derived
>Derived : Derived

        2?: Base; // error
>2 : Base
>Base : Base
    }

    interface A3 {
>A3 : A3

        '1'?: Base;
>'1' : Base
>Base : Base

        '2.0'?: Derived;
>'2.0' : Derived
>Derived : Derived
    }

    interface B3 extends A3 {
