=== tests/cases/conformance/types/typeRelationships/subtypesAndSuperTypes/subtypingWithObjectMembersOptionality2.ts ===
// Derived member is optional but base member is not, should be an error

interface Base { foo: string; }
>Base : Base
>foo : string

interface Derived extends Base { bar: string; }
>Derived : Derived
>Base : Base
>bar : string

interface T {
>T : T

    Foo: Base;
>Foo : Base
>Base : Base
}

interface S extends T {
>S : S
>T : T

    Foo?: Derived // error
>Foo : Derived
>Derived : Derived
}

interface T2 {
>T2 : T2

    1: Base;
>1 : Base
>Base : Base
}

interface S2 extends T2 {
>S2 : S2
>T2 : T2

    1?: Derived; // error
>1 : Derived
>Derived : Derived
}

interface T3 {
>T3 : T3

    '1': Base;
>'1' : Base
>Base : Base
}

interface S3 extends T3 {
>S3 : S3
>T3 : T3

    '1'?: Derived; // error
>'1' : Derived
>Derived : Derived
}

// object literal case
var a: { Foo: Base; }
>a : { Foo: Base; }
>Foo : Base
>Base : Base

var b: { Foo?: Derived; }
>b : { Foo?: Derived; }
>Foo : Derived
>Derived : Derived

var r = true ? a : b; // ok
>r : { Foo: Base; } | { Foo?: Derived; }
>true ? a : b : { Foo: Base; } | { Foo?: Derived; }
>true : true
>a : { Foo: Base; }
>b : { Foo?: Derived; }

