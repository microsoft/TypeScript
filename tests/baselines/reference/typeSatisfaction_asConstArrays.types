// with readonly array
const arr1 = [1, 2, 3] as const satisfies readonly unknown[]
>arr1 : readonly [1, 2, 3]
>     : ^^^^^^^^^^^^^^^^^^
>[1, 2, 3] as const satisfies readonly unknown[] : readonly [1, 2, 3]
>                                                : ^^^^^^^^^^^^^^^^^^
>[1, 2, 3] as const : readonly [1, 2, 3]
>                   : ^^^^^^^^^^^^^^^^^^
>[1, 2, 3] : readonly [1, 2, 3]
>          : ^^^^^^^^^^^^^^^^^^
>1 : 1
>  : ^
>2 : 2
>  : ^
>3 : 3
>  : ^

