=== tests/cases/compiler/underscoreThisInDerivedClass02.ts ===
// @target es5

// Original test intent:
// Errors on '_this' should be reported in derived constructors,
// even if 'super()' is not called.

class C {
>C : C

    constructor() {
        return {};
>{} : {}
    }
}

class D extends C {
>D : D
>C : C

    constructor() {
        super();
>super() : void
>super : typeof C

        var _this = "uh-oh?";
>_this : string
>"uh-oh?" : "uh-oh?"
    }
}
