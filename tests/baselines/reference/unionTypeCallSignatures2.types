interface A {
    (x: number): number;
>x : number
>  : ^^^^^^

    (x: string, y?: string): boolean;
>x : string
>  : ^^^^^^
>y : string
>  : ^^^^^^

    (x: Date): void;
>x : Date
>  : ^^^^

    <T>(x: T[]): T[];
>x : T[]
>  : ^^^
}

interface B {
    (x: number): number;
>x : number
>  : ^^^^^^

    (x: string): string;
>x : string
>  : ^^^^^^

    (x: Date): void;
>x : Date
>  : ^^^^

    <T>(x: T[]): T[];
>x : T[]
>  : ^^^
}

interface C {
    (x: string, ...y: string[]): number;
>x : string
>  : ^^^^^^
>y : string[]
>  : ^^^^^^^^

    (x: number, s?: string): number;
>x : number
>  : ^^^^^^
>s : string
>  : ^^^^^^

    <T>(x: T[]): T[];
>x : T[]
>  : ^^^
}

var f1: A | B | C;
>f1 : A | B | C
>   : ^^^^^^^^^

var n1 = f1(42);             // number
>n1 : number
>   : ^^^^^^
>f1(42) : number
>       : ^^^^^^
>f1 : A | B | C
>   : ^^^^^^^^^
>42 : 42
>   : ^^

var s1 = f1("abc");          // boolean | string | number
>s1 : string | number | boolean
>   : ^^^^^^^^^^^^^^^^^^^^^^^^^
>f1("abc") : string | number | boolean
>          : ^^^^^^^^^^^^^^^^^^^^^^^^^
>f1 : A | B | C
>   : ^^^^^^^^^
>"abc" : "abc"
>      : ^^^^^

var a1 = f1([true, false]);  // boolean[]
>a1 : boolean[]
>   : ^^^^^^^^^
>f1([true, false]) : boolean[]
>                  : ^^^^^^^^^
>f1 : A | B | C
>   : ^^^^^^^^^
>[true, false] : boolean[]
>              : ^^^^^^^^^
>true : true
>     : ^^^^
>false : false
>      : ^^^^^

