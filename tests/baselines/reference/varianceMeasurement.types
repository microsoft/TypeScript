// The type below should be invariant in T but is measured as covariant because
// we don't analyze recursive references.

interface Foo1<T> {
  x: T;
>x : T

  y: Foo1<(arg: T) => void>;
>y : Foo1<(arg: T) => void>
>arg : T
}

declare const f10: Foo1<string>;
>f10 : Foo1<string>

const f11: Foo1<'a'> = f10;
>f11 : Foo1<"a">
>f10 : Foo1<string>

const f12: Foo1<unknown> = f10;
>f12 : Foo1<unknown>
>f10 : Foo1<string>

